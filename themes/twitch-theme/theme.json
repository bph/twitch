{
	"$schema": "http://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"color": {
			"defaultPalette": false,
			"palette": [
				{
					"slug": "orange",
					"color": "#e76f51",
					"name": "Orange"
				},
				{
					"slug": "dark-grey",
					"color": "#264653",
					"name": "Dark Gray"
				}
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "'Montserrat', sans-serif",
					"name": "Primary",
					"slug": "primary"
				}
			]
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--dark-grey)",
			"text": "var(--wp--preset--color--white)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--primary)"
		},
		"elements": {
			"h1": {
				"color": {
					"background": "var(--wp--preset--color--orange)",
					"text": "var(--wp--preset--color--dark-grey)"
				}
			}
		}
	},
	"customTemplates": [
		{
			"name": "page-landing",
			"title": "Page (Landing)",
			"postTypes": [
				"page"
			]
		}
	]
}
