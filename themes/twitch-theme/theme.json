{
	"$schema": "http://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"layout": {
			"contentSize": "650px",
			"wideSize": "1000px"
		},
		"color": {
			"defaultPalette": false,
			"palette": [
				{
					"slug": "orange",
					"color": "#e76f51",
					"name": "Orange"
				},
				{
					"slug": "dark-gray",
					"color": "#264653",
					"name": "Dark Gray"
				}
			],
			"gradients": [
				{
					"slug": "orange-to-dark-gray",
					"gradient": "linear-gradient(180deg,var(--wp--preset--color--orange) 33%,var(--wp--preset--color--dark-gray) 33%)",
					"name": "Orange to Dark Gray"
				}
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"slug": "primary",
					"fontFamily": "'Montserrat', sans-serif",
					"name": "Primary"
				}
			]
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--dark-gray)",
			"text": "var(--wp--preset--color--white)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--primary)"
		},
		"elements": {
			"h1": {
				"color": {
					"background": "var(--wp--preset--color--orange)",
					"text": "var(--wp--preset--color--dark-gray)"
				}
			}
		},
		"blocks": {
			"core/navigation": {
				"color": {
					"text": "var(--wp--preset--color--white)"
				},
				"typography": {
					"fontSize": "1.2rem",
					"fontWeight": "bold"
				}
			}
		}
	},
	"customTemplates": [
		{
			"name": "page-landing",
			"title": "Page (Landing)",
			"postTypes": [
				"page"
			]
		}
	]
}
